@model ChooseUsOptions;

@if (Model != null)
{
	<div class="section mt-6">

		<div class="container">

			<div class="heading-block mt-4 text-center">

				<h3>@Html.Raw(Model.Title)</h3>

			</div>

			<div class="row justify-content-center col-mb-50">

				@Html.DisplayFor(model => model.Items)

			</div>

		</div>

	</div>
}